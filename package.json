    "update": "run-os",
    "update:win32": "npm-run-all update:win:*",
    "update:win:autoload": "composer dump-autoload",
    "update:linux:darwin": "npm-run-all update:nix:*",
    "update:nix:autoload": "bin/composer dump-autoload"
