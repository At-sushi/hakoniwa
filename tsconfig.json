{
    "compilerOptions": {
        "listEmittedFiles": true,
        "module": "commonjs",
        "newLine": "lf",
        "noEmitOnError": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "preserveConstEnums": true,
        "pretty": true,
        "removeComments": true,
        "strict": true,
        "sourceMap": true,
        "target": "ES2017"
    },
}
